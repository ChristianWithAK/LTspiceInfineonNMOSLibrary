.PARAM  UT=26m     ab=24.8m    lB=-23       UB=66

.PARAM  b0=101.7   p0=6.808    p1=-22.7m    p2=35.3u

.PARAM  Rd=8.54m   nmu=3.81    Tref=298     T0=273      lnIsj=-28.08
.PARAM  ndi=1.027  nisj=0	   Rdi=1.84m    nmu2=0.649
.PARAM  td=10n     ta=10n
.PARAM  Rf=0.705   nmu3=1.2

.PARAM  kbq=86.2u

* Cgs
.PARAM  f3=620p    rc1=50p

* Cgfp
.PARAM  f3a=36.0p

* Cds
.PARAM q81=97.74p
.PARAM qs1=51.4p   qs2=81.2p   qs3=-17.7m
.PARAM qs4=237p    qs5=-94.9m
.PARAM x0=13.68    x1=28.34    dx={x1-x0}
.PARAM f2r=0.4

* Cgd
.PARAM  ps0=21.6p ps1=71.3p ps2=-0.138 ps3=41.8p ps4=-1.23 ps5=2.84p ps6=6.06p
.PARAM  f5=15.84p
.PARAM  x2=9.42   x3=24.81     dx2={x3-x2}

.PARAM  Vth={Vth0+dVthmax*dVth}
