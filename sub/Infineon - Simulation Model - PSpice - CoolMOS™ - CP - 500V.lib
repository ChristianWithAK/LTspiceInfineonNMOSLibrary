.SUBCKT cool_500_f_var dd g s Tj t1 PARAMS: act=1 dVth=0 dR=0 Inn=1u Unn=10 Rmax=1 Rs=1u heat=1

*control parameter: 1 if diode should store charge, 0 otherwise
.PARAM enable_diode=1
     
.PARAM w0={0.1p+SQRT(act)*0.8p}   w1={116p*act} w2={9p*act}  x1=-1    x2=-80m  
.PARAM Uoff=0.25    y1={exp(Uoff*x1)}
.PARAM w3={400p*act}  x3=-500m 
.PARAM w4={60p*act}   x4=1
.PARAM w7={0.5p*act}  w8={300p*act}  x8=-70m                                
.PARAM Cgs={115p*act} Cox={w0+w1+w2}
.PARAM k14=-2   deltb=1

.PARAM L=2u            g2=57.5m        fpar1=110m      fpar2=0.95
.PARAM Vth0=3.85       fpar29=300           Tref=273        fpar3=5.5m
.PARAM fpar4=500m       fpar5=100p        fpar6=550         coxi=431.4u
.PARAM fpar7=207m      W={175m*act}    fpar9=5
.PARAM fpar10=2.22        tt=450n
.PARAM fpar12=1            fpar13={2.15/act}  fpar14={55m/act}   fpar15=11
.PARAM fpar18=85.8u       fpar19=-29        fpar20=1.23         fpar21=10.8p
.PARAM fpar22={W*coxi*g2/L}
 
.PARAM Vmin=2.95     Vmax=4.85         
.PARAM Vth={Vth0+(Vmax-Vth0)*limit(dVth,0,1)-(Vmin-Vth0)*limit(dVth,-1,0)}
.PARAM p1={Unn-Inn*Rs-Vth0}
.PARAM p2={(((p1-SQRT(p1**2-4*fpar2/fpar22*Inn*(1+fpar1*p1)))/fpar2*fpar12+1)**2-1)/(4*fpar12)}
.PARAM p3={fpar15/(2*(Inn*(Inn*(Rmax-Rs)-p2)))}
.PARAM Rlim={p3*(-fpar15+SQRT(fpar15**2+4*(Rmax*Inn)**2-8*Rmax*Inn*(Inn*Rs+p2)+(2*p2+2*Inn*Rs)**2))}
.PARAM dRd={fpar13+if(dVth==0,limit(dR,0,1)*max(Rlim-fpar13,0),0)}

.FUNC  fpar24(Uee,p,pp,z1)  {if(Uee>pp,(Uee-fpar2*z1)*z1,p*(pp-p)/fpar2*exp((Uee-pp)/p))}
.FUNC  fpar25(Uds,p,Uee,z1,Tjx) {(fpar22/(1+fpar1*Uee)*(fpar29/Tjx)**1.5)*fpar24(Uee,p,min(2*p,p+fpar2*Uds),z1)}
.FUNC  fpar28(Uds,Ugs,Tjx,p)  {fpar25(Uds,p,Ugs-Vth+fpar3*(Tjx-fpar29),min(Uds,(Ugs-Vth+fpar3*(Tjx-fpar29))/(2*fpar2)),Tjx)}
.FUNC  fpar26(Uds,Tjx)      {act*exp(min(fpar19+(Uds-fpar6-fpar4*(Tjx-fpar29))/fpar7,23))}
.FUNC  fpar27(Uds,Ugs,Tjx) {sgn(Uds)*fpar28((SQRT(1+4*fpar12*abs(Uds))-1)/2/fpar12,Ugs,Tjx,fpar9*fpar18*Tjx)}

R1 g s 1G
G_G1 d s VALUE={fpar27(V(d,s),V(g,s),Tref+limit(V(Tj),-200,999))}
Rd01 d s 500Meg

V_Ichannel d1 d 0
V_Iepi     dd d2 0  
G_G_Rd     d2 d1 VALUE {V(d2,d1)/((dRd*0.5*(1+SQRT(1+4*(max(V(d2,d1),0)/fpar15)**2)))
+                       *((Tref+LIMIT(V(t1),-200,999))/fpar29)**fpar10)}
R_R_ERd_g  d2 d1  10k

G_Rdiod    dd    dio2 VALUE {V(dd,dio2)/(fpar14*((Tref+LIMIT(V(t1),-200,999))/fpar29)**1.4)}
R_Rdiod    dd    dio 500Meg
V_Idiode   dio2  dio 0

*body diode does not have dynamic temperature behaviour in order to improve commutation convergence
Dbody      s    di    DBODY
.MODEL DBODY  D(IS={fpar21*act}  N={fpar20}  RS=2u  EG=1.12  TT={tt})

Rd02       s    di 500Meg
G_diode2   s    di VALUE={-fpar26(V(di,s),Tref+LIMIT(V(t1),-200,999))}
V_Idio     dio  di 0

C_pack    dd     g {w0}
E_Edg1    d1   ox1 VALUE {V(d1,g)-((exp(min((V(d1,g)+Uoff)*x1,0))-y1)/x1+min((V(d1,g)+Uoff),0))}
C_Cdg1    ox1    g {w1}
E_Edg2    d1   ox2 VALUE {V(d1,g)-((exp(min(V(d1,g)*x2,0))-1)/x2+min(V(d1,g),0))}
C_Cdg2    ox2    g {w2}

E_Eds1  dio  dEQJ1 VALUE {V(dio,s)-((exp(max(V(dio,s),-3)*x3)-1)/x3)}
C_Cds1  dEQJ1    s {w3}
E_Eds2  d1   dEQJ2 VALUE {V(d1,s)-2*(SQRT(x4*limit(x4+V(d1,s),0,2*fpar6))-x4)}
C_Cds2  dEQJ2    s {w4}
E_Eds3  dio  dEQJ3 VALUE {V(dio,s)-((exp(max(V(dio,s),-3)*x8)-1)/x8)}
C_Cds3  dEQJ3    s {w8}
C_Cds4  d1       s {w7}
  
C_Cgs   g        s {Cgs}

G_G_Ptot_channel 0 Tj VALUE {heat*LIMIT(V(d,s)*I(V_Ichannel),0,100k) }
G_G_Ptot_Epi     0 t1 VALUE {heat*(LIMIT(V(dd,d1)*I(V_Iepi),0,100k)
+                            +LIMIT(V(dd,dio)*I(V_Idiode)+V(dio,s)*I(V_Idio),0,100k))}

.ENDS
