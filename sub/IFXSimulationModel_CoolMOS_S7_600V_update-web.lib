.PARAM	Tref=298
.PARAM	fpar1={4.63-0.01}
.PARAM	fpar2={4.69m+0.5m}
.PARAM	fpar3={-3.2m*1}
.PARAM  fpar4={11.7*1.28}
.PARAM  fpar5={0.46*1.17}
.PARAM  fpar6={3.12*1.056}
.PARAM  fpar7={0.3*1.18}
.PARAM  fpar8={6.40*1}
.PARAM  fpar9={0.9*1.0}
